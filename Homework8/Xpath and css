Example 1
// document.getElementById
// document.getElementById("top_slider")
// <div id=​"top_slider">​
// 	<div class=​"bx-wrapper" style=​"max-width:​ 100%;​">​
// 		<div class=​"bx-viewport" style=​"width:​ 100%;​ overflow:​ hidden;​ position:​ relative;​ height:​ 824px;​">​…​</div>
// ​			<div class=​"bx-controls bx-has-pager bx-has-controls-direction">​…​</div>
// ​	</div>​
// </div>​

	// XPath
	document.evaluate('//*[@id="top_slider"]', document)
	XPathResult {resultType: 4, invalidIteratorState: false}
	// CSS
	document.querySelector('#top_slider')
	<div id=​"top_slider">​<div class=​"bx-wrapper" style=​"max-width:​ 100%;​">​<div class=​"bx-viewport" style=​"width:​ 100%;​ overflow:​ hidden;​ position:​ relative;​ height:​ 452px;​">​…​</div>​<div class=​"bx-controls bx-has-pager bx-has-controls-direction">​…​</div>​</div>​</div>​

Example 2
// document.getElementsByName
// document.getElementsByName("RegistrationUser[ruType]")
// NodeList(3) [input, input, input]0: input1: input2: inputlength: 3__proto__: NodeList

	// XPath
	document.evaluate('//*[@name="RegistrationUser[ruType]"]', document)
	XPathResult {resultType: 4, invalidIteratorState: false}
	// CSS
	document.querySelectorAll('input[name="RegistrationUser[ruType]"]')
	NodeList(3) [input, input, input]

